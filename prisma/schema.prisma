generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model passingStats {
  id          Int       @id @default(autoincrement())
  date        DateTime? @default(dbgenerated("(CURRENT_TIMESTAMP AT TIME ZONE 'UTC'::text)")) @db.Timestamp(6)
  Team        String    @db.VarChar(25)
  Att         Decimal   @db.Decimal
  Cmp         Decimal   @db.Decimal
  Cmp_perc    Decimal   @db.Decimal
  Yds_per_Att Decimal   @db.Decimal
  Pass_Yds    Decimal   @db.Decimal
  TD          Decimal   @db.Decimal
  INT         Decimal   @db.Decimal
  Rate        Decimal   @db.Decimal
  first       Decimal   @db.Decimal
  first_perc  Decimal   @db.Decimal
  over_20     Decimal   @db.Decimal
  over_40     Decimal   @db.Decimal
  Lng         Decimal   @db.Decimal
  Sck         Decimal   @db.Decimal
  SckY        Decimal   @db.Decimal
}

model test {
  id Int @id @default(autoincrement())
}
